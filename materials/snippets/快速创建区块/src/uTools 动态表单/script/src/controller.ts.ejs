export type MethodHandle = (data: {
  method: string;
  params: string;
  model: object;
  scriptFile: string;
}) => Promise<{
  /** 立即更新 model */
  updateModelImmediately?: boolean;
  /** 仅更新参数 */
  onlyUpdateParams?: boolean;
  params?: string;
  model: object;
}>;

export const initFiltersFromImage: MethodHandle = (data) => {
  return Promise.resolve({
    updateModelImmediately: false,
    model: data.model,
    onlyUpdateParams: true,
    params: 'JFKDJFKDJK',
  });
};
